<!-- Views/State/Index.cshtml -->

@model IEnumerable<ElectionApp.Models.State>

<h1>Number of MP Seats in Each State</h1>

<table class="table">
    <thead>
        <tr>
            <th>State</th>
            <th>Number of MP Seats</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var state in Model)
        {
            <tr>
                <td>@state.Name</td>
                <td>@state.NumberOfMPSeats</td>
            </tr>
        }
    </tbody>
</table>
